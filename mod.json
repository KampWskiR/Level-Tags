{
	"geode": "4.0.1",
	"gd": {
		"win": "2.2074",
		"mac": "2.2074",
		"android": "2.2074"
	},
	"version": "v0.2.0-alpha",
	"id": "kampwski.level_tags",
	"name": "Level Tags",
	"developer": "KampWski",
	"description": "Mod adding tags to levels",
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": "v1.17.0",
			"importance": "required"
		}
	],
	"tags": [
		"interface",
		"utility",
		"online"
	],
	"repository": "https://github.com/KampWskiR/Level-Tags",
	"resources": {
		"sprites": [
			"resources/*.png",
			"resources/tags/*.png"
		],
		"spritesheets": {
			"tags": [
				"resources/tags/*.png"
			]
		}
	},
	"settings": {
		"levelcellShow": {
			"name": "LevelCell Tags",
			"description": "Enable Tags displaying in LevelCell",
			"type": "bool",
			"default": true
		},
		"levelInfoShow": {
			"name": "LevelInfoLayer Tags",
			"description": "Enable tags displaying in LevelInfoLayer",
			"type": "bool",
			"default": false
		}
	}
}